    <TextView
        android:id="@+id/textView5"
        android:layout_width="265dp"
        android:layout_height="62dp"
        android:text="Start on all your fours with wrists under shoulders and knees down under hips. Your back should be straight and your neck should be neutral."
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/imageView19"
        app:layout_constraintVertical_bias="0.111" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="251dp"
        android:layout_height="103dp"
        android:text="Brace your core and draw your shoulder blades down your back toward your hips. to start the move, simultaneously straighten and raise your left leg and right arm, keeping your pelvis and shoulders in a neutral position. Donâ€™t  raise or lower your head. hold for 2 seconds."
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.198" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="254dp"
        android:layout_height="57dp"
        android:layout_marginEnd="76dp"
        android:text="Bend and lower your leg and arm down back to the starting positio nwhile maintaining stability. Then switch,raising your right leg and left arm"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.375" />

